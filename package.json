{
  "name": "cap-type-generator",
  "version": "1.0.0",
  "description": "Testing CAP with @cap-js/cds-typer",
  "repository": "",
  "license": "UNLICENSED",
  "private": false,
  "dependencies": {
    "@sap/cds": "^7",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0.19.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@tsconfig/node20": "^20.1.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "start": "cds-serve",
    "build": "run-s build:cds build:ts",
    "build:cds": "cds build --production",
    "build:ts": "tsc && cp -r @cds-models gen/srv",
    "gen:types": "npx @cap-js/cds-typer \"*\" --outputDirectory @cds-models",
    "watch": "cds-ts watch"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}
