{
  "name": "cap-type-generator",
  "version": "1.0.0",
  "description": "Testing CAP with @cap-js/cds-typer",
  "repository": "",
  "license": "UNLICENSED",
  "private": false,
  "dependencies": {
    "@cap-js/hana": "^1.2.0",
    "@sap/cds": "^8",
    "@sap/xssec": "^4",
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "https://github.com/cap-js/cds-typer.git#fix/build-task-output",
    "@cap-js/cds-types": "^0.6.5",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.13",
    "@types/node": "^20",
    "axios": "^1.7.7",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "start": "cds-serve",
    "build": "rimraf mta_archives && npx -p mbt mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "copy:types": "cp -r @cds-models gen/srv",
    "watch": "cds-ts watch",
    "cf:de": "cf de cap-type-generator-srv",
    "test": "jest --silent"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "typer": {
      "useEntitiesProxy": true
    },
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "db": "hana"
      }
    }
  }
}
